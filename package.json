{
  "name": "vue3-admin",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "style:fix": "stylelint \"src/**/*.(vue|less|css)\" --fix",
    "eslint:fix": "eslint --fix --ext .js,.vue src",
    "prettier:fix": "prettier --write ./src/../*.{less,js,json,.vue}",
    "format:all": "npm run style:fix && npm run eslint:fix && npm run prettier:fix",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "commit": "git-cz"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "element-plus": "^1.2.0-beta.3",
    "number-precision": "^1.5.1",
    "vue": "^3.2.16",
    "vue-router": "4",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.1.1",
    "git-cz": "^4.8.0",
    "husky": "^7.0.0",
    "less": "^4.1.2",
    "lint-staged": "^12.0.3",
    "prettier": "^2.4.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^5.0.0",
    "unplugin-vue-components": "^0.17.2",
    "vite": "^2.6.4",
    "vite-plugin-style-import": "^1.3.0"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "npm run format:all",
      "git add ."
    ]
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
}
